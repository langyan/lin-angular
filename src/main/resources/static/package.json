  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "DEBUG": false,
      "VERSION": false,
      "CONFIG": false
    },
    "rules":{
    "linebreak-style": 0
  }
  }
